{
  "expo": {
    "name": "TasbeehApp",
    "slug": "TasbeehApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ]
      },
      "icon": "./assets/icon.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "icon": "./assets/icon.png",
      "permissions": [
        "NOTIFICATIONS",
        "android.permission.VIBRATE",
        "android.permission.INTERNET"
      ],
      "package": "com.omer310.TasbeehApp",
      "useNextNotificationsApi": true
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icon1.png",
          "color": "#ffffff",
          "sounds": [
            "./assets/adhan.mp3",
            "./assets/madinah_adhan.mp3",
            "./assets/makkah_adhan.mp3"
          ]
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ],
      [
        "expo-background-fetch",
        {
          "backgroundFetch": true
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "0871aeac-9dd1-4614-b6e6-7614e6b8cade"
      }
    },
    "fonts": [
      {
        "asset": "./assets/fonts/Scheherazade-Regular.ttf",
        "family": "Scheherazade"
      }
    ],
    "assetBundlePatterns": [
      "**/*",
      "assets/quran-images/*"
    ]
  }
}
